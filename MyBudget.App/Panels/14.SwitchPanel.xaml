<?xml version="1.0" encoding="utf-8" ?>
<ContentView
    x:Class="SampleApp.Panels.SwitchPanel"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    BackgroundColor="{DynamicResource BackgroundColor}"
    xmlns:md="clr-namespace:Material.Components.Maui;assembly=Material.Components.Maui">
    
    <VerticalStackLayout HorizontalOptions="Center" VerticalOptions="Center">
        <md:Switch />
        <md:Switch HasIcon="False" />
        <md:Switch IsEnabled="False" />
        <md:Switch IsChecked="True" IsEnabled="False" />

    </VerticalStackLayout>
</ContentView>